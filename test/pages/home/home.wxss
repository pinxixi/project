/**index.wxss**/
.container{
  width: 750rpx;
  height: 100%;
}
/*轮播图背景*/
.container .bg{
  position: relative;
  width: 100%;
}
.container .bg image{
  width: 100%;
}
/*名字*/
.container .title{
  position: absolute;
  top: 0;
  left: 0;
  width: 100%;
  color: #fff;
  font-size: 38rpx;
}
/*轮播图*/
.swiper{
  position: absolute;
  top: 98rpx;
  left: 56rpx;
  width:629rpx;
  height:360rpx;
  margin:10rpx auto;
  border-radius: 30rpx;
  overflow: hidden;
  margin-bottom:103rpx;
}
.swiper .swiperImg{
  width:700rpx;
  height:400rpx;
}
/*轮播左右两边的竖条*/
.vertical{
  position: absolute;
  top: 90rpx;
  width: 32rpx;
  height: 390rpx;
  background: #bfbfbf;
}
.verticalBarL{
  left: 0;
  border-radius: 0 30rpx 30rpx 0;
}
.verticalBarR{
  right: 0;
  border-radius:  30rpx 0 0 30rpx;
}
/*拼团信息*/
.join{
  width: 100%;
  margin-top: 104rpx;
}
.joinBg{
  position: relative;
  width: 100%;
  height: 257rpx;
}
.joinBg image{
  width: 100%;
  height: 100%;
}
.joinBg text{
  position: absolute;
  top: 30rpx;
  left: 50%;
  transform: translateX(-50%);
  color: #fff;
  font-size: 30rpx;
}
/*开始拼团*/
.startJoin{
  position: absolute;
  top: 668rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 658rpx;
  height: 284rpx;
  text-align: left;
  background: #fff;
  border-radius: 23rpx;
  padding-top: 56rpx;
  box-shadow: 3rpx 5rpx 15rpx 10rpx rgba(0,0,0,.4);
}
.joinInfo image{
  float: left;
  width: 280rpx;
  height: 200rpx;
  margin-left: 10rpx;
  margin-right: 10rpx;
}
.joinInfo .info{
  height: 284rpx;
}
.joinInfo .info .infoDes{
  width: 100%;
  font-size: 26rpx;
}
.price{
  width: 100%;
  margin-top: 24rpx;
}
.price .newPrice{
  font-size: 14rpx;
  color: #ff3000;
  margin-right: 8rpx;
}
.price .newPrice text{
  font-size: 24rpx;
}
.price .oldPrice{
  font-size: 14rpx;
  color: #b4b4b4;
  text-decoration: line-through;
}
.info .timeShop{
  float: left;
  width: 109rpx;
  height: 32rpx;
  text-align: center;
  line-height: 32rpx;
  margin-top: 28rpx;
  padding: 5rpx;
  font-size: 20rpx;
  color: #ff4c22;
  border: 2rpx solid #ff4c22;
}
/*团长信息*/
.imgJoin{
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 230rpx;
}
.imgJoin view:first-child{
  position: relative;
}
.imgJoin view:first-child text{
  position: absolute;
  bottom: 0;
  left: 50%;
  transform: translateX(-50%);
  width: 42rpx;
  height: 14rpx;
  text-align: center;
  line-height: 14rpx;
  font-size: 14rpx;
  color: #fff;
  padding: 2rpx 10rpx 2rpx 10rpx;
  border-radius: 20rpx;
  background: #ff3000;
}
.imgJoin view{
  width: 70rpx;
  height: 70rpx;
  margin-right: 20rpx;
}
.imgJoin view image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.imgJoin .emptyImg{
  display: flex;
  justify-content: center;
  align-items: center;
  width: 70rpx;
  height: 70rpx;
  background: #bababa;
  color: #fff;
  border-radius: 50%;
  border: 1rpx dashed #5e5e5e;
}
/*剩余数量*/
.count{
  width: 100%;
  margin-top: 44rpx;
  text-align: center;
  font-size: 23rpx;
  color: #252525;
}
.count text text{
  color: #cd0202;
}
/*剩余时间*/
.time{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100%;
  height: 31rpx;
  margin-top: 38rpx;
  font-size: 23rpx;
}
.time .line{
  width: 208rpx;
  height: 2rpx;
  background: #c7c7c7;
}
.countdown{
  flex:1;
  color:#545454;
  text-align: center;
}
.timer{
  display:inline-block;
  width:34rpx;
  height:31rpx;
  margin:4rpx;
  text-align: center;
  color:#f40000;
  background: #fd897d;
}
/*玩趣加购*/
.play{
  display: flex;
  flex-direction: column;
  width: 100%;
  margin-top: 80rpx;
}
.play .img{
  display: flex;
  justify-content: center;
  width: 100%;
}
.play .img image{
  width: 344rpx;
  height: 30rpx;
}
.play .playText{
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 40rpx;
  font-size: 36rpx;
  color: #333;
}
.play .text{
  display: flex;
  justify-content: center;
  width: 100%;
  margin-top: 26rpx;
  font-size: 32rpx;
  color: #b9b9b9;
}
/*图片*/
.pic{
